<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="aiGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#059669;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#10B981;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="300" fill="url(#aiGradient)"/>
  
  <!-- Chat header -->
  <rect x="20" y="20" width="360" height="50" rx="5" fill="white" opacity="0.9"/>
  <rect x="30" y="30" width="80" height="30" rx="3" fill="#059669"/>
  <circle cx="350" cy="45" r="8" fill="#10B981"/>
  
  <!-- Chat messages area -->
  <rect x="20" y="80" width="360" height="160" rx="5" fill="white" opacity="0.8"/>
  
  <!-- User message -->
  <rect x="200" y="100" width="150" height="30" rx="15" fill="#3B82F6"/>
  <text x="275" y="120" text-anchor="middle" fill="white" font-family="Arial" font-size="12">Hello AI!</text>
  
  <!-- AI response -->
  <rect x="50" y="140" width="200" height="40" rx="15" fill="#F3F4F6"/>
  <text x="150" y="160" text-anchor="middle" fill="#374151" font-family="Arial" font-size="12">Hi! How can I help you today?</text>
  
  <!-- User message 2 -->
  <rect x="180" y="190" width="160" height="30" rx="15" fill="#3B82F6"/>
  <text x="260" y="210" text-anchor="middle" fill="white" font-family="Arial" font-size="12">Tell me about AI</text>
  
  <!-- Input area -->
  <rect x="20" y="250" width="300" height="40" rx="20" fill="white" opacity="0.9"/>
  <rect x="30" y="260" width="280" height="20" rx="10" fill="#F3F4F6"/>
  <rect x="320" y="250" width="60" height="40" rx="20" fill="#059669"/>
  <text x="350" y="275" text-anchor="middle" fill="white" font-family="Arial" font-size="12">Send</text>
</svg>
