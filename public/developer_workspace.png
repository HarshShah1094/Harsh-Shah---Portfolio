<svg width="600" height="400" viewBox="0 0 600 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="workspaceGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1E40AF;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#7C3AED;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#059669;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="600" height="400" fill="url(#workspaceGradient)"/>
  
  <!-- Monitor -->
  <rect x="100" y="80" width="200" height="120" rx="10" fill="#1F2937"/>
  <rect x="110" y="90" width="180" height="100" fill="#111827"/>
  <rect x="190" y="200" width="20" height="30" fill="#374151"/>
  <rect x="150" y="230" width="100" height="8" rx="4" fill="#374151"/>
  
  <!-- Code lines on screen -->
  <rect x="120" y="110" width="60" height="4" fill="#10B981"/>
  <rect x="120" y="120" width="80" height="4" fill="#3B82F6"/>
  <rect x="120" y="130" width="70" height="4" fill="#F59E0B"/>
  <rect x="120" y="140" width="90" height="4" fill="#EF4444"/>
  <rect x="120" y="150" width="50" height="4" fill="#8B5CF6"/>
  
  <!-- Keyboard -->
  <rect x="120" y="250" width="160" height="40" rx="5" fill="#374151"/>
  <rect x="130" y="260" width="140" height="20" rx="3" fill="#1F2937"/>
  
  <!-- Coffee cup -->
  <rect x="350" y="120" width="30" height="40" rx="15" fill="#92400E"/>
  <rect x="355" y="125" width="20" height="30" rx="10" fill="#FEF3C7"/>
  <rect x="380" y="130" width="8" height="20" rx="4" fill="#92400E"/>
  
  <!-- Books -->
  <rect x="320" y="200" width="8" height="60" fill="#DC2626"/>
  <rect x="330" y="190" width="8" height="70" fill="#059669"/>
  <rect x="340" y="210" width="8" height="50" fill="#7C3AED"/>
  
  <!-- Plant -->
  <rect x="450" y="180" width="6" height="40" fill="#16A34A"/>
  <circle cx="453" cy="170" r="15" fill="#22C55E"/>
  <circle cx="460" cy="175" r="12" fill="#16A34A"/>
  <circle cx="445" cy="175" r="10" fill="#15803D"/>
  
  <!-- Floating elements -->
  <circle cx="500" cy="100" r="3" fill="#F59E0B" opacity="0.7"/>
  <circle cx="520" cy="150" r="2" fill="#EF4444" opacity="0.7"/>
  <circle cx="480" cy="200" r="4" fill="#8B5CF6" opacity="0.7"/>
  <circle cx="530" cy="250" r="2" fill="#10B981" opacity="0.7"/>
</svg>
